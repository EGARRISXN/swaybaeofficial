{
  "name": "swaybaeofficial",
  "version": "1.0.0",
  "license": "MIT",
  "description": "The Sway Bae Official Website. Built with Next.js, the Sanity CLI, TailwindCSS, and more.",
  "keywords": [
    "Sanity.io",
    "NextJS",
    "TailwindCSS"
  ],
  "private": true,
  "homepage": "https://swaybae.net",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --check --ignore-path .gitignore .",
    "format:fix": "prettier --write --ignore-path .gitignore ."
  },
  "dependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/typography": "^0.5.10",
    "@vercel/analytics": "^1.1.2",
    "@vercel/speed-insights": "^1.0.9",
    "autoprefixer": "^10.4.17",
    "daisyui": "3.2.1",
    "framer-motion": "^10.18.0",
    "hamburger-react": "^2.5.0",
    "next": "^14.1.0",
    "next-themes": "^0.2.1",
    "nodemailer": "^6.9.9",
    "postcss": "^8.4.33",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.0.1",
    "sharp": "^0.32.6",
    "tailwindcss": "^3.4.1"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "eslint-config-next": "^13.5.6",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.2.4",
    "prettier-plugin-tailwindcss": "^0.5.11"
  },
  "prettier": {
    "semi": false,
    "tabWidth": 2,
    "printWidth": 100,
    "bracketSpacing": false,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "plugins": [
      "prettier-plugin-tailwindcss"
    ]
  }
}
